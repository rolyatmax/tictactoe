{
  "name": "tictactoe-ui",
  "version": "0.0.1",
  "homepage": "https://github.com/rolyatmax/tictactoe",
  "authors": [
    "Taylor Baldwin <taylorbaldwin@gmail.com>"
  ],
  "license": "MIT",
  "main": "js/main.js",
  "scripts": {
    "build": "mkdir -p build && browserify . -o build/build.js",
    "watch": "mkdir -p build && watchify . -o build/build.js -dv",
    "lint": "eslint ."
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "backbone": "^1.2.3",
    "jquery": "^3.5.1",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "markdownify": "^0.1.0",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      "markdownify"
    ]
  }
}
